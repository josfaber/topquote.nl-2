<!DOCTYPE html>
<html lang="nl">
	<head>
		{% block head %}
			<meta http-equiv="X-UA-Compatible" content="IE=edge">
			<meta name="viewport" content="width=device-width, initial-scale=1">
			<link rel="profile" href="http://gmpg.org/xfn/11"/>
			<link rel="dns-prefetch" href="//use.typekit.net">
			<link rel="preconnect" href="https://use.typekit.net/" crossorigin>
			<style>{{inlinecss}}</style>
			{% for css_file in css_files %}
				<link rel="preload" href="{{ css_file }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
				<noscript><link rel="stylesheet" href="{{ css_file }}"></noscript>
			{% endfor %}
			<title>
				{% block title %}{% endblock %}
				- topquote</title>
			<link rel="shortcut icon" href="/favicon.ico">
		{% endblock %}
		<script id="cookieyes" type="text/javascript" src="https://cdn-cookieyes.com/client_data/2b1dc6bf53e833741ae1003a/script.js"></script>

		<script async src="https://www.googletagmanager.com/gtag/js?id=G-6YFKGSMEFN"></script>
		<script>
			window.dataLayer = window.dataLayer || [];
			function gtag() {
			dataLayer.push(arguments);
			}
			gtag('js', new Date());

			gtag('config', 'G-6YFKGSMEFN', {'anonymize_ip': true});
		</script>
		<script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-2356098750828124" crossorigin="anonymous"></script>
		<!-- Matomo -->
		<script>
		var _paq = window._paq = window._paq || [];
		/* tracker methods like "setCustomDimension" should be called before "trackPageView" */
		_paq.push(['trackPageView']);
		_paq.push(['enableLinkTracking']);
		(function() {
			var u="//analytics.mmsrv.nl/";
			_paq.push(['setTrackerUrl', u+'matomo.php']);
			_paq.push(['setSiteId', '2']);
			var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
			g.async=true; g.src=u+'matomo.js'; s.parentNode.insertBefore(g,s);
		})();
		</script>
		<!-- End Matomo Code -->
	</head>

	<body class="{% if is_home %}home{% endif %}{% if is_single_quote %} single-quote {% endif %} {% if constant('ENVIRONMENT') == 'development' %}dev{% endif %} {% block extra_body_classes %}{% endblock %}">
		<nav>
			{% block nav %}
				{{ include('partials/nav.html.twig') }}
			{% endblock %}
		</nav>
		<main>
			{% block content %}
				content
			{% endblock %}
		</main>
		<div id="terms-container" class="terms-container">
			<label for="terms">Zoek</label>
			<input type="text" id="terms">
		</div>
		<div id="share-container" class="share-container">
			<div id="share-url" class="share-url">{{ tqd.site_url }}</div>
			<div class="share-buttons">
				<span id="btnCopy" class="icon-copy" data-action="copy"></span>
				<span id="btnTweet" class="icon-twitter" data-action="twitter"></span>
				<span id="btnMail" class="icon-mail" data-action="mail"></span>
			</div>
		</div>
		<div id="loader-container" class="loader-container">
			<span class="icon-spinner"></span>
		</div>
		{% block scripts %}
			<script>
				window.tqd = {
is_search:{% if is_search %}true
{% else %}
false
{% endif %},
rsk: '{{ RECAPTCHA_SITE_KEY }}',{% for key, value in tqd %}{{ key }} : '{{ value }}',{% endfor %}{% for key, value in tqd_num %}{{ key }} : {{ value }},{% endfor %}
};
			</script>
			{% for js_file in js_files %}
				<script defer src="{{ js_file}}"></script>
			{% endfor %}
		{% endblock %}

	</body>
</html>
